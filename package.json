{
  "name": "@guardian/interactive-component-library",
  "private": false,
  "version": "2.0.4",
  "packageManager": "pnpm@8.4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/guardian/interactive-component-library"
  },
  "main": "./dist/interactive-component-library.umd.js",
  "module": "./dist/interactive-component-library.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/interactive-component-library.es.js",
      "require": "./dist/interactive-component-library.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "engines": {
    "node": "18"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "storybook build",
    "build:lib": "tsc && vite build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,mdx}\"",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "prepare": "husky install",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\""
  },
  "dependencies": {
    "preact": "^10.12.1"
  },
  "devDependencies": {
    "@babel/core": "7.22.10",
    "@storybook/addon-essentials": "7.2.2",
    "@storybook/addon-interactions": "7.2.2",
    "@storybook/addon-links": "7.2.2",
    "@storybook/addon-styling": "1.3.6",
    "@storybook/blocks": "7.2.2",
    "@storybook/preact": "7.2.2",
    "@storybook/preact-vite": "7.2.2",
    "@storybook/testing-library": "0.2.0",
    "@testing-library/preact": "^3.2.3",
    "@types/node": "20.4.9",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@preact/preset-vite": "^2.8.0",
    "@vitest/coverage-v8": "0.34.1",
    "autoprefixer": "10.4.14",
    "babel-loader": "9.1.3",
    "eslint": "8.46.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard-with-typescript": "37.0.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-n": "16.0.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-storybook": "0.6.13",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "json": "11.0.0",
    "lint-staged": "13.2.3",
    "postcss": "8.4.33",
    "prettier": "3.0.1",
    "prop-types": "15.8.1",
    "storybook": "7.2.2",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "vite": "^4.0.0",
    "vite-plugin-dts": "3.5.1",
    "vitest": "0.34.1"
  },
  "peerDependencies": {
    "preact": "^10.12.1"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier -w"
    ]
  }
}
