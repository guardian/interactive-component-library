    "@storybook/addon-measure": "^7.6.16",
    "@storybook/addon-storysource": "^7.6.16",
    "@storybook/addon-styling": "1.3.7",
    "@storybook/addon-viewport": "^7.6.16",
    "@storybook/blocks": "7.6.16",
    "@storybook/preact": "7.6.16",
    "@storybook/preact-vite": "7.6.16",
